{
  "name": "@tkvw/api",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "author": "Dennie de Lange",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.js"
    },
    "./*": {
      "require": "./lib/*.cjs",
      "import": "./lib/*.js"
    },
    "./fs": {
      "require": "./lib/fs/index.cjs",
      "import": "./lib/fs/index.js"
    },
    "./object": {
      "require": "./lib/object/index.cjs",
      "import": "./lib/object/index.js"
    }
  },
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "lib/*"
      ]
    }
  },
  "scripts": {
    "build": "heft build --clean",
    "test": "heft test --clean",
    "watch": "heft build --clean --watch"
  },
  "devDependencies": {
    "@tkvw/eslint-config": "workspace:*",
    "@tkvw/heft-node-rig": "workspace:*",
    "eslint": "~8.4.1",
    "@rushstack/heft": "~0.43.2"
  }
}
